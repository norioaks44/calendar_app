<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<div class="calendar-part">

  <h2><%= link_to '活動日を登録する', new_event_path %></h2>

<%= month_calendar events: @events do |date, events| %>
  <%= date.day %>

  <% events.each do |event| %>
    <div>
      <%= event.title.name %>
    </div>
  <% end %>

<% end %>

</div>